<?xml version="1.0" encoding="utf-8" ?>

<model xmlns="http://www.top-logic.com/ns/dynamic-types/6.0">
	<module name="prodElement">
		<annotations>
			<singletons>
				<singleton type="RootElement"/>
			</singletons>
		</annotations>
		<class name="RootElement">
			<annotations>
				<table name="StructuredElement"/>
				<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
			</annotations>
			<attributes>
				<property name="name"
					mandatory="true"
					type="tl.core:String"
				>
					<annotations>
						<delete-protected/>

						<!-- Singletons must be created without additional input. -->
						<default-value>
							<string value="ROOT"/>
						</default-value>
					</annotations>
				</property>
				<reference name="children"
					override="true"
					type="RootElementChild"
				/>
			</attributes>
			<generalizations>
				<generalization type="prodElement.RootElement"/>
				<generalization type="tl.element:StructuredElement"/>
				<generalization type="tl.element:StructuredElementContainer"/>
			</generalizations>
		</class>
		<interface name="prodElement.RootElement">
			<annotations>
				<instance-presentation
					expanded-icon="/prod/iO.png"
					icon="/prod/i.png"
				/>
				<legacy-qualified-name qualified-name="prodElement.RootElement"/>
			</annotations>
		</interface>
		<class name="Product">
			<annotations>
				<table name="Product"/>
				<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
			</annotations>
			<attributes>
				<reference name="children"
					override="true"
					type="ProductChild"
				/>
			</attributes>
			<generalizations>
				<generalization type="prodElement.Product"/>
				<generalization type="tl.element:StructuredElement"/>
				<generalization type="prodElement:RootElementChild"/>
				<generalization type="tl.element:StructuredElementContainer"/>
			</generalizations>
		</class>
		<interface name="prodElement.Product">
			<annotations>
				<instance-presentation
					expanded-icon="/prod/iO.png"
					icon="/prod/i.png"
				/>
				<legacy-qualified-name qualified-name="prodElement.Product"/>
			</annotations>
			<generalizations>
				<generalization type="prodElement.RootElement"/>
			</generalizations>
		</interface>
		<class name="ProductType">
			<annotations>
				<table name="ProductType"/>
				<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
			</annotations>
			<generalizations>
				<generalization type="prodElement.ProductType"/>
				<generalization type="prodElement:ProductChild"/>
				<generalization type="tl.element:StructuredElement"/>
			</generalizations>
		</class>
		<interface name="prodElement.ProductType">
			<annotations>
				<instance-presentation
					expanded-icon="/prod/iO.png"
					icon="/prod/i.png"
				/>
				<legacy-qualified-name qualified-name="prodElement.ProductType"/>
			</annotations>
			<generalizations>
				<generalization type="prodElement.Product"/>
			</generalizations>
		</interface>
		<class name="ProductVersion">
			<annotations>
				<table name="ProductVersion"/>
				<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
			</annotations>
			<generalizations>
				<generalization type="prodElement.ProductVersion"/>
				<generalization type="prodElement:ProductChild"/>
				<generalization type="tl.element:StructuredElement"/>
			</generalizations>
		</class>
		<interface name="prodElement.ProductVersion">
			<annotations>
				<instance-presentation
					expanded-icon="/prod/iO.png"
					icon="/prod/i.png"
				/>
				<legacy-qualified-name qualified-name="prodElement.ProductVersion"/>
			</annotations>
			<generalizations>
				<generalization type="prodElement.Product"/>
			</generalizations>
		</interface>
		<interface name="RootElementChild">
			<generalizations>
				<generalization type="tl.element:StructuredElement"/>
			</generalizations>
			<attributes>
				<reference name="parent"
					kind="backwards"
					override="true"
					type="RootElement"
				/>
			</attributes>
		</interface>
		<interface name="ProductChild">
			<generalizations>
				<generalization type="tl.element:StructuredElement"/>
			</generalizations>
			<attributes>
				<reference name="parent"
					kind="backwards"
					override="true"
					type="Product"
				/>
			</attributes>
		</interface>
	</module>
</model>