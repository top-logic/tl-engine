<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE taglib
    PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
            
    "http://java.sun.com/j2ee/dtd/web-jsptaglibrary_1_2.dtd">
<!--
	The tag library for forms replace regular HTML forms.

	Forms created with this tag library superseed regular HTML forms
	in the following ways:

     - Well-formedness is guaranteed at compile time of the JSP.
     - Automatic input processing through {@link com.top_logic.layout.form.model.FormContext}.
     - Automatic handling of form-related AJAX actions.

 -->
<taglib>

  <!-- The version number of this tag library -->
  <tlib-version>1.0</tlib-version>

  <!-- The JSP specification version required to function -->
  <jsp-version>1.2</jsp-version>

  <!-- The short name of this tag library -->
  <short-name>ajaxform</short-name>

  <!-- General description of this tag library -->
  <description>This tag library provides HTML forms that are AJAX-aware.</description>

  <tag>
    <name>page</name>
    <tag-class>com.top_logic.layout.form.tag.PageAreaTag</tag-class>
    <description>
		Structuring element rendering a page with header and body area.
    </description>
    <attribute>
	    <name>pageRenderer</name>
    	<required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>
        	Custom renderer for writing the page structure (theme provides default).
        </description>
    </attribute>
    <attribute>
	    <name>titleView</name>
    	<required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>
        	Custom view/control rendering the title contents.
        </description>
    </attribute>
    <attribute>
	    <name>subTitleView</name>
    	<required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>
        	Custom view/control rendering the sub-title contents.
        </description>
    </attribute>
    <attribute>
	    <name>iconBarView</name>
    	<required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>
        	Custom view/control rendering the icon bar contents.
        </description>
    </attribute>
    <attribute>
	    <name>bodyView</name>
    	<required>false</required>
        <rtexprvalue>true</rtexprvalue>
        <description>
        	Custom view/control rendering the body contents.
        </description>
    </attribute>
  </tag>
  
  <tag>
    <name>title</name>
    <tag-class>com.top_logic.layout.form.tag.PageTitleTag</tag-class>
    <description>
		Contents of the title area of a page header, see `form:page`.
    </description>
  </tag>
  
  <tag>
    <name>subtitle</name>
