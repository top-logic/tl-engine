			clique="create"
			confirm="false"
			disabledImage=""
			group="Create"
			image=""
			target="selection(self())"
			targetComponent="com.top_logic.demo/technical/components/polymorphicForms/newObject/dialog.layout.xml#Editor"
		>
			<resourceKey key="dynamic.4440e981-bafb-4bd4-83fd-3d6cfa541f08">
				<en>New object</en>
				<de>Neues Objekt</de>
			</resourceKey>
		</open-handler>
