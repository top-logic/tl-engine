<?xml version="1.0" encoding="utf-8"?>

<!-- 
	Configuration options for testing tlservicejms with JUnit.
 -->
<application xmlns:config="http://www.top-logic.com/ns/config/6.0">
	<configs>
		<config config:interface="test.com.top_logic.basic.util.AbstractBasicTestAll$GlobalConfig"
			test-comment="true"
			test-layouts-normalized="true"
			test-jsp-content="true"
		/>
		
		<config config:interface="test.com.top_logic.basic.TestComment$Config">
			<copyright-pattern>\W*Copyright \([c,C]\) (\d\d\d\d(?:\s?-\s?\d\d\d\d)?) \QBusiness Operation Systems GmbH\E\. All Rights Reserved</copyright-pattern>
		</config>
	</configs>
</application>