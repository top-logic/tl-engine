<?xml version="1.0" encoding="utf-8"?>

<!-- Main configuration file -->
<root>
	<alias>
        <entry name="%PAC_CALLBACK_URL%" value="${env:tl_pac_callback_url:/servlet/callback}" />
        <entry name="%PAC_CALLBACK_ABSOLUTE%" value="${env:tl_pac_callback_absolute:false}" />
        <entry name="%PAC_CLIENT_ID%" value="${env:tl_pac_client_id}" />
        <entry name="%PAC_SECRET%" value="${env:tl_pac_secret}" />
		<entry name="%PAC_CLIENT_AUTHENTICATION_METHOD%" value="${env:tl_pac_client_authentication_method:client_secret_basic}" />
		<entry name="%PAC_SCOPE%" value="${env:tl_pac_scope}" />
		<entry name="%PAC_ACR_VALUES%" value="${env:tl_pac_acr_values}" />
	</alias>

	<properties>
	</properties>
</root>
