<?xml version="1.0" encoding="utf-8" ?>

<!-- Example how to use an EditableTree -->
<layout>
	<components>
		<component class="com.top_logic.demo.knowledge.test.layout.DynamicTreeDemo"
			buttonbar="true"
			model="provider(com.top_logic.layout.component.model.SelfModelProvider)"
			name="editDynamicTreeDemo"
			page="/jsp/layout/test/DynamicTree.jsp"
			resPrefix="layouts.layoutdemo.tree.dynamicTree.editDynamicTreeDemo."
			securityProviderClass="securityRoot"
		>
			<commands>
				<command id="delNode"
					class="com.top_logic.demo.knowledge.test.layout.DynamicTreeDemo$DelNodeCommand"
				/>
				<command id="addNode"
					class="com.top_logic.demo.knowledge.test.layout.DynamicTreeDemo$AddNodeCommand"
				/>
			</commands>
			<buttons>
				<button id="setUserObject"
					class="com.top_logic.demo.knowledge.test.layout.DynamicTreeDemo$SetUserObjectCommand"
				/>
			</buttons>
		</component>
	</components>
</layout>