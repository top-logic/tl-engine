<?xml version="1.0" encoding="utf-8"?>

<application xmlns:config="http://www.top-logic.com/ns/config/6.0">
	<services>
		<config service-class="com.top_logic.gui.JSFileCompiler">
			<instance>
				<additional-files>
					<!--
						Note: The client-side module script cannot be included from the client modules, 
						since these do not not depend on tl-basic and therefore cannot refer to the 
						file compiler configuration. -->
					<file config:operation="remove" resource="/TLGraphClientDiagramJS/TLGraphClientDiagramJS.nocache.js" />
					<file resource="/TLBlocksClient/TLBlocksClient.nocache.js" />
				</additional-files>
			</instance>
		</config>
		
		<config service-class="com.top_logic.model.search.expr.config.SearchBuilder">
			<instance class="com.top_logic.model.search.expr.config.SearchBuilder">
				<methods>
					<java name="flowChart" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowChart"/>
					<java name="flowEmpty" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowEmpty"/>
					<java name="flowText" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowText"/>
					<java name="flowImage" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowImage"/>
					<java name="flowAlign" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowAlign"/>
					<java name="flowBorder" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowBorder"/>
					<java name="flowFill" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowFill"/>
					<java name="flowStack" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowStack"/>
					<java name="flowPadding" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowPadding"/>
					<java name="flowSelection" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowSelection"/>
					<java name="flowClickTarget" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowClickTarget"/>
					<java name="flowTooltip" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowTooltip"/>
					<java name="flowHorizontal" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowHorizontal"/>
					<java name="flowVertical" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowVertical"/>
					<java name="flowCompass" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowCompass"/>
					<java name="flowGrid" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowGrid"/>
					<java name="flowTree" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowTree"/>
					<java name="flowConnection" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowConnection"/>
					<java name="flowConnector" method="com.top_logic.graphic.flow.server.script.FlowFactory#flowConnector"/>
				</methods>
			</instance>
		</config>
	</services>

	<configs>
	</configs>
</application>
