        <config service-class="com.top_logic.basic.format.configured.FormatterService">
			<instance class="com.top_logic.basic.format.configured.FormatterService">
				<lenient>true</lenient>
				<rounding-mode>HALF_UP</rounding-mode>
				
				<formats>
					<number id="number"
						style="number"
					/>
					<number id="long"
						result-type="long"
						style="integer"
					/>
					<decimal id="double"
						normalize="true" 
						pattern="#,##0.00" 
						result-type="double"
					/>
					
					<normalizing-format id="percent">
						<format class="com.top_logic.basic.format.NumberFormatDefinition" style="percent" />
						
						<parser class="com.top_logic.basic.format.PercentFormatDefinition">
							<format class="com.top_logic.basic.format.NumberFormatDefinition" style="number" />
						</parser>
					</normalizing-format>

					<date id="date"
						lenient="default"
						style="medium"
					/>
					<!--
						Must use custom date for Excel date, because POI 
						needs a SimpleDateFormat which is only build for a custom date 
					-->
					<custom-date id="excelDate"
						pattern="dd.MM.yyyy"
					/>
					<time id="time"
						lenient="default"
						style="medium"
					/>
					<date-time id="date-time"
						date-style="medium"
						lenient="default"
						time-style="medium"
					/>
					
					<date-time id="medium-date-time">
						<date-style>medium</date-style>
						<time-style>short</time-style>
						<lenient>default</lenient>
					</date-time>
					
					<date id="short-date">
						<style>short</style>
						<lenient>default</lenient>
					</date>
					<time id="short-time">
						<style>short</style>
						<lenient>default</lenient>
					</time>
					<date-time id="short-date-time">
						<date-style>short</date-style>
						<time-style>short</time-style>
						<lenient>default</lenient>
					</date-time>
					
					<decimal id="decimal"
						normalize="true" 
						pattern="###,###,###,###,##0.00"
					/>
					<decimal id="integer"
						result-type="long" 
						pattern="###,###,###,###,##0"
					/>
					<decimal id="percentage"
						normalize="true" 
						pattern="###,###,###,###,##0.00 %"
					/>
				</formats>
			</instance>
		</config>

