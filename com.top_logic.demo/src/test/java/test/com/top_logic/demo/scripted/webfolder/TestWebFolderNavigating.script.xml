					component-implementation-comment="com.top_logic.element.layout.structured.AdminElementComponent"
					component-name="com.top_logic.demo/structure/demoTypes/demoTypesView.layout.xml#DemoTypesEdit"
					user-id="root"
				>
					<arguments/>
				</action>
			</actions>
		</action>
