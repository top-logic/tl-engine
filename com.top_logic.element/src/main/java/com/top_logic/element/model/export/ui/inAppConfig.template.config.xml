<?xml version="1.0" encoding="utf-8" ?>

<application>
	<services>
		<config service-class="com.top_logic.basic.util.ResourcesModule">
			<instance>
				<bundles>
					<bundle name="${bundleName}"/>
				</bundles>
			</instance>
		</config>

		<config service-class="com.top_logic.util.model.ModelService">
			<instance>
				<declarations>
					<declaration file="${modelPath}"/>
				</declarations>
			</instance>
		</config>
	</services>
</application>