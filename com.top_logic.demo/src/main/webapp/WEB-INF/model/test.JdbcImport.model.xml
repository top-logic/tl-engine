<?xml version="1.0" ?>
<model xmlns:config="http://www.top-logic.com/ns/config/6.0">
	<module name="test.JdbcImport">
		<class name="A">
			<annotations>
				<table-binding name="A" primary-key="ID"></table-binding>
			</annotations>
			<attributes>
				<property name="int" type="tl.core:Integer">
					<annotations>
						<column-binding name="int"></column-binding>
					</annotations>
				</property>
				<property name="str" type="tl.core:String">
					<annotations>
						<column-binding name="STR"></column-binding>
					</annotations>
				</property>
				<property mandatory="true" name="id" type="tl.core:Integer">
					<annotations>
						<column-binding name="ID"></column-binding>
						<visibility value="hidden"></visibility>
					</annotations>
				</property>
				<reference name="cs" type="C" navigate="true" multiple="true" ordered="true" kind="forwards">
					<annotations>
						<reverse-foreign-key-binding target-columns="A" order-column="ORDER"/>
					</annotations>
				</reference>
				<reference name="bs" type="B" navigate="true" multiple="true" kind="forwards">
					<annotations>
						<link-table-binding link-table="LINK" source-columns="A" destination-columns="B"/>
					</annotations>
				</reference>
			</attributes>
		</class>
		<class name="B">
			<annotations>
				<table-binding name="B" primary-key="MYID"></table-binding>
			</annotations>
			<attributes>
				<property mandatory="true" name="myid"
					type="tl.core:Integer">
					<annotations>
						<column-binding name="MYID"></column-binding>
						<visibility value="hidden"></visibility>
					</annotations>
				</property>
				<reference name="aref" type="A" navigate="true"
					kind="forwards">
					<annotations>
						<foreign-key-binding columns="AREF"></foreign-key-binding>
					</annotations>
				</reference>
				<reference name="other" type="B" navigate="true"
					kind="forwards">
					<annotations>
						<foreign-key-binding columns="OTHER"></foreign-key-binding>
					</annotations>
				</reference>
			</attributes>
		</class>
		<class name="C">
			<annotations>
				<table-binding name="C"></table-binding>
			</annotations>
			<attributes>
				<property name="value"
					type="tl.core:String">
					<annotations>
						<column-binding name="VAL"></column-binding>
					</annotations>
				</property>
			</attributes>
		</class>
	</module>
</model>