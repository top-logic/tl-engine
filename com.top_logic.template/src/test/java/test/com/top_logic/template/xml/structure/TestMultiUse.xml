<?xml version='1.0' encoding='utf-8'?>

<t:template xmlns:t="http://www.top-logic.com/ns/template/1.0">
	<t:head>
		<t:settings output-format="text" />
		<t:types>
			
			<t:type name="Example_Type">
				<t:attributes>
					<t:attribute name="Example_Attribute" type="string"/>
				</t:attributes>
			</t:type>
			
		</t:types>
		<t:parameters>
			
			<t:parameter name="Example_Structure_A" type="Example_Type" />
			
			<t:parameter name="Example_Structure_B" type="Example_Type" />
			
			<t:parameter name="Example_Structure_C" type="Example_Type" />
			
		</t:parameters>
	</t:head>
	<t:body>
		
		<t:value expr="$Example_Structure_A.Example_Attribute" />
		
		<t:value expr="$Example_Structure_B.Example_Attribute" />
		
		<t:value expr="$Example_Structure_C.Example_Attribute" />
		
	</t:body>
</t:template>
