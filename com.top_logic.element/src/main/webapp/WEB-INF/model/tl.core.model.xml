									<derived-storage>
										<expression-evaluation>
											<method-call method="com.top_logic.knowledge.service.Revision#resolveDate()"/>
										</expression-evaluation>
									</derived-storage>
								</storage-algorithm>
								<delete-protected/>
								<format>
									<format-ref format-id="date-time"/>
								</format>
							</annotations>
						</property>
						<property name="author"
							type="tl.core:Author"
