<?xml version="1.0" encoding="utf-8" ?>

<template>
	<params>
		<param name="structure"/>
		<param name="defaultFor"
			optional="true"
		/>
		<param name="page"
			optional="true"
		/>
		<param name="createCommandGroups"
			optional="true"
		/>
		<param name="editCommandGroups"
			optional="true"
		/>
		<param name="editDialogs"
			optional="true"
		/>
		<param name="openCreateCommandGroup"
			optional="true"
		/>
		<param name="securityProvider"
			optional="true"
		/>
		<param name="modifier"
			optional="true"
		/>
		<param name="subConfiguration"
			optional="true"
		/>
		<param name="exportDialogs"
			optional="true"
		/>
	</params>

	<include name="editStructure.xml"
		createCommandGroups="${createCommandGroups}"
		defaultFor="${defaultFor}"
		editCommandGroups="${editCommandGroups}"
		editDialogs="${editDialogs}"
		exportDialogs="${exportDialogs}"
		modifier="${modifier}"
		openCreateCommandGroup="${openCreateCommandGroup}"
		page="${page}"
		rootVisible="false"
		securityProvider="${securityProvider}"
		showNoModel="true"
		structure="${structure}"
		subConfiguration="${subConfiguration}"
	/>
</template>