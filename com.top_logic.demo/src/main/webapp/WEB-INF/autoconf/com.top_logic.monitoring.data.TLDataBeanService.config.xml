<?xml version="1.0" encoding="utf-8" ?>

<application config:interface="com.top_logic.basic.config.ApplicationConfig$Config"
	xmlns:config="http://www.top-logic.com/ns/config/6.0"
>
	<services>
		<config service-class="com.top_logic.monitoring.data.TLDataBeanService"
			config:override="true"
		>
			<instance class="com.top_logic.monitoring.data.TLDataBeanService">
				<mbeans>
					<mbean class="com.top_logic.monitoring.data.MonitorUser"/>
					<mbean class="com.top_logic.monitoring.data.MonitorDatabase"/>
					<mbean class="com.top_logic.monitoring.data.MonitorByExpression"
						impl="true"
						name="com.top_logic.monitoring.data:name=expression0"
					/>
					<mbean class="com.top_logic.monitoring.data.MonitorByExpression"
						impl="1"
						name="com.top_logic.monitoring.data:name=expression1"
					/>
				</mbeans>
			</instance>
		</config>
	</services>
</application>