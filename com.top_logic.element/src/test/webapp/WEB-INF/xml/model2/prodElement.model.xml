<?xml version="1.0" encoding="utf-8" ?>

<model xmlns="http://www.top-logic.com/ns/dynamic-types/6.0">
	<module name="prodElement">
		<annotations>
			<singletons>
				<singleton type="RootElement"/>
			</singletons>
		</annotations>
		<interface name="Test.Global.SubSub">
			<annotations>
				<legacy-qualified-name qualified-name="Test.Global.SubSub"/>
			</annotations>
			<generalizations>
				<generalization type="Test.Global"/>
			</generalizations>
		</interface>
		<class name="RootElement"
			abstract="true"
		>
			<annotations>
				<table name="StructuredElement"/>
				<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
			</annotations>
			<attributes>
				<property name="name"
					mandatory="true"
					type="tl.core:String"
				>
					<annotations>
						<delete-protected/>

						<!-- Singletons must be created without additional input. -->
						<default-value>
							<string value="ROOT"/>
						</default-value>
					</annotations>
				</property>
				<reference name="children"
					override="true"
					type="RootElementChild"
				/>
			</attributes>
			<generalizations>
				<generalization type="tl.element:StructuredElementContainer"/>
			</generalizations>
			<types>
				<class name="RootElement">
					<annotations>
						<table name="StructuredElement"/>
						<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
					</annotations>
					<generalizations>
						<generalization
							scope="this"
							type="prodElement.RootElement"
						/>
						<generalization type="RootElement"/>
						<generalization type="tl.element:StructuredElement"/>
					</generalizations>
					<types>
						<interface name="prodElement.RootElement">
							<annotations>
								<instance-presentation
									expanded-icon="/prod/iO.png"
									icon="/prod/i.png"
								/>
								<legacy-qualified-name qualified-name="prodElement.RootElement"/>
							</annotations>
						</interface>
					</types>
				</class>
			</types>
		</class>
		<class name="Product"
			abstract="true"
		>
			<annotations>
				<table name="Product"/>
				<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
				<create-scope
					create-type="Product"
					scope-ref="this"
				/>
			</annotations>
			<attributes>
				<reference name="children"
					override="true"
					type="ProductChild"
				/>
			</attributes>
			<generalizations>
				<generalization type="prodElement:RootElementChild"/>
				<generalization type="tl.element:StructuredElementContainer"/>
			</generalizations>
			<types>
				<class name="Product">
					<annotations>
						<table name="Product"/>
						<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
					</annotations>
					<generalizations>
						<generalization
							scope="this"
							type="prodElement.Product"
						/>
						<generalization type="Product"/>
						<generalization type="tl.element:StructuredElement"/>
					</generalizations>
					<types>
						<interface name="prodElement.Product">
							<annotations>
								<instance-presentation
									expanded-icon="/prod/iO.png"
									icon="/prod/i.png"
								/>
								<legacy-qualified-name qualified-name="prodElement.Product"/>
							</annotations>
							<generalizations>
								<generalization
									scope="parent"
									type="prodElement.RootElement"
								/>
							</generalizations>
						</interface>
					</types>
				</class>
			</types>
		</class>
		<class name="ProductType"
			abstract="true"
		>
			<annotations>
				<table name="ProductType"/>
				<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
				<create-scope
					create-type="ProductType"
					scope-ref="this"
				/>
			</annotations>
			<generalizations>
				<generalization type="prodElement:ProductChild"/>
				<generalization type="tl.element:StructuredElement"/>
			</generalizations>
			<types>
				<class name="ProductType">
					<annotations>
						<table name="ProductType"/>
						<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
					</annotations>
					<generalizations>
						<generalization
							scope="this"
							type="prodElement.ProductType"
						/>
						<generalization type="ProductType"/>
						<generalization type="tl.element:StructuredElement"/>
					</generalizations>
					<types>
						<interface name="prodElement.ProductType">
							<annotations>
								<instance-presentation
									expanded-icon="/prod/iO.png"
									icon="/prod/i.png"
								/>
								<legacy-qualified-name qualified-name="prodElement.ProductType"/>
							</annotations>
							<generalizations>
								<generalization
									scope="parent"
									type="prodElement.Product"
								/>
							</generalizations>
						</interface>
					</types>
				</class>
			</types>
		</class>
		<class name="ProductVersion"
			abstract="true"
		>
			<annotations>
				<table name="ProductVersion"/>
				<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
				<create-scope
					create-type="ProductVersion"
					scope-ref="this"
				/>
			</annotations>
			<generalizations>
				<generalization type="prodElement:ProductChild"/>
				<generalization type="tl.element:StructuredElement"/>
			</generalizations>
			<types>
				<class name="ProductVersion">
					<annotations>
						<table name="ProductVersion"/>
						<implementation-binding class-name="com.top_logic.element.structured.wrap.AttributedStructuredElementWrapper"/>
					</annotations>
					<generalizations>
						<generalization
							scope="this"
							type="prodElement.ProductVersion"
						/>
						<generalization type="ProductVersion"/>
						<generalization type="tl.element:StructuredElement"/>
					</generalizations>
					<types>
						<interface name="prodElement.ProductVersion">
							<annotations>
								<instance-presentation
									expanded-icon="/prod/iO.png"
									icon="/prod/i.png"
								/>
								<legacy-qualified-name qualified-name="prodElement.ProductVersion"/>
							</annotations>
							<generalizations>
								<generalization
									scope="parent"
									type="prodElement.Product"
								/>
							</generalizations>
						</interface>
					</types>
				</class>
			</types>
		</class>
		<interface name="RootElementChild">
			<generalizations>
				<generalization type="tl.element:StructuredElement"/>
			</generalizations>
			<attributes>
				<reference name="parent"
					kind="backwards"
					override="true"
					type="RootElement"
				/>
			</attributes>
		</interface>
		<interface name="ProductChild">
			<generalizations>
				<generalization type="tl.element:StructuredElement"/>
			</generalizations>
			<attributes>
				<reference name="parent"
					kind="backwards"
					override="true"
					type="Product"
				/>
			</attributes>
		</interface>
	</module>
</model>