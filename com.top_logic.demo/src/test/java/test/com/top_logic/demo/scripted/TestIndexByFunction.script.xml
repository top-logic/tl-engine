  // Test with empty list
  emptyResult = list().indexBy(
    keyFun: x -> $x,
    mapFun: x -> toUpperCase($x)
  );
  
  // Test with list containing null values
  mixedData = list(
    {"key": "A", "value": "test1"},
    {"key": "B", "value": null},
    {"key": "C", "value": "test2"}
  );
  
  resultWithNull = $mixedData.indexBy(
    keyFun: item -> $item["key"],
