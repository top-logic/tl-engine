		<typeOptions
			include-subtypes="true"
			type="test.polymorphism:A"
		/>
		<create-handler id="ID_4d02a979_7e50_4bba_bef2_10212e117f71"
			clique="save"
			confirm="false"
			disabledImage=""
			group="Create"
			ignoreWarnings="default"
			image=""
			linkOperation=""
		/>
	</arguments>
</config:template-call>