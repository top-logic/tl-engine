<?xml version="1.0" encoding="utf-8" ?>

<model xmlns="http://www.top-logic.com/ns/dynamic-types/6.0">
	<module name="test.flowchart">
		<class name="FlowNode">
			<generalizations>
				<generalization type="tl.model:TLObject"/>
			</generalizations>
			<attributes>
				<property name="name"
					mandatory="true"
					type="tl.core:String"
				/>
				<reference name="inputs"
					inverse-reference="usedIn"
					kind="forwards"
					multiple="true"
					navigate="true"
					ordered="true"
					type="FlowNode"
				>
					<annotations>
						<constraints>
							<constraint class="com.top_logic.model.util.NoAttributeCycle"/>
						</constraints>
					</annotations>
				</reference>
				<reference name="usedIn"
					inverse-reference="inputs"
					kind="backwards"
					multiple="true"
					navigate="true"
					type="FlowNode"
				/>
				<property name="image"
					type="tl.core:Binary"
				/>
				<reference name="locations"
					composite="true"
					kind="forwards"
					multiple="true"
					navigate="true"
					type="Location"
				/>
				<property name="marker"
					type="tl.core:String"
				/>
			</attributes>
		</class>
		<class name="Location">
			<generalizations>
				<generalization type="tl.model:TLObject"/>
			</generalizations>
			<attributes>
				<reference name="part"
					kind="forwards"
					mandatory="true"
					navigate="true"
					type="Part"
				/>
				<reference name="usages"
					kind="forwards"
					multiple="true"
					navigate="true"
					type="Usage"
				/>
				<property name="count"
					type="tl.core:Integer"
				/>
				<property name="name"
					type="tl.core:String"
				>
					<annotations>
						<storage-algorithm>
							<query expr="x -> $x.get(`test.flowchart:Location#part`).toString()"/>
						</storage-algorithm>
					</annotations>
				</property>
			</attributes>
		</class>
		<class name="Part">
			<generalizations>
				<generalization type="tl.model:TLObject"/>
			</generalizations>
			<attributes>
				<property name="name"
					mandatory="true"
					type="tl.core:String"
				/>
				<property name="color"
					type="tl.util:Color"
				/>
			</attributes>
		</class>
		<class name="Usage">
			<generalizations>
				<generalization type="tl.model:TLObject"/>
			</generalizations>
			<attributes>
				<property name="name"
					mandatory="true"
					type="tl.core:String"
				/>
			</attributes>
		</class>
	</module>
</model>