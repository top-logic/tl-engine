<?xml version="1.0" encoding="utf-8" ?>

<model xmlns="http://www.top-logic.com/ns/dynamic-types/6.0">
	<module name="test.process">
		<class name="Project">
			<generalizations>
				<generalization type="tl.model:TLObject"/>
			</generalizations>
			<annotations>
				<form-definition>
					<form columns="ONE">
						<field
							attribute="name"
							fullQualifiedName="test.process:Project#name"
							type="tl.core:String"
						/>
					</form>
				</form-definition>
			</annotations>
			<attributes>
				<property name="name"
					mandatory="true"
					type="tl.core:String"
				/>
				<reference name="tickets"
					composite="true"
					kind="forwards"
					multiple="true"
					navigate="true"
					type="Ticket"
				/>
			</attributes>
		</class>
		<class name="Ticket">
			<generalizations>
				<generalization type="tl.bpe.execution:ProcessExecution"/>
			</generalizations>
			<annotations>
				<form-definition>
					<form columns="ONE">
						<field
							attribute="name"
							fullQualifiedName="test.process:Ticket#name"
							type="tl.core:String"
						/>
						<field
							attribute="description"
							fullQualifiedName="test.process:Ticket#description"
							type="tl.model.wysiwyg:Html"
						/>
					</form>
				</form-definition>
			</annotations>
			<attributes>
				<property name="name"
					mandatory="true"
					type="tl.core:String"
				/>
				<property name="description"
					type="tl.model.wysiwyg:Html"
				/>
			</attributes>
		</class>
	</module>
</model>