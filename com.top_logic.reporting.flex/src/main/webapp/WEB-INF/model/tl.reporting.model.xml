<?xml version="1.0" encoding="utf-8" ?>

<model xmlns="http://www.top-logic.com/ns/dynamic-types/6.0">
	<module name="tl.reporting">
		<class name="FlexReport">
			<annotations>
				<table name="FlexReport"/>
			</annotations>
			<generalizations>
				<generalization type="tl.accounts:WithOwner"/>
				<generalization type="tl.search:IsPublished"/>
			</generalizations>
			<attributes>
				<property name="name"
					mandatory="true"
					type="tl.core:String"
				/>
				<property name="types"
					mandatory="true"
					type="tl.core:String"
				>
					<annotations>
						<storage-algorithm>
							<primitive-storage storage-attribute="bo_type"/>
						</storage-algorithm>
					</annotations>
				</property>
				<property name="version"
					mandatory="true"
					type="tl.core:Double"
				/>
				<property name="report"
					mandatory="true"
					type="tl.core:String"
				>
					<annotations>
						<size-constraint/>
					</annotations>
				</property>
			</attributes>
		</class>
	</module>
</model>